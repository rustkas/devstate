{
  "name": "@rustkas/devstate-client",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "gen": "openapi-generator-cli generate -i ../openapi.yaml -g typescript-axios -o src --additional-properties=supportsES6=true,withSeparateModelsAndApi=true,modelPropertyNaming=original",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rustkas/devstat.git"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "openapi-generator-cli": "^2.12.1"
  },
  "dependencies": {
    "axios": "^1.7.2"
  }
}